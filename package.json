{
  "name": "@blinkk/degu",
  "version": "2.28.1",
  "description": "A general utility library for interactive websites, with a focus on performance.",
  "author": "Blinkk <hello@blinkk.com>",
  "type": "module",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "default": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./lib/*": {
      "default": "./lib/*"
    }
  },
  "directories": {
    "lib": "lib"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blinkkcode/degu.git"
  },
  "files": [
    "lib"
  ],
  "ava": {
    "require": [
      "ts-node/register/transpile-only",
      "./_ava/_setup-browser-env.cjs"
    ],
    "extensions": [
      "ts"
    ],
    "files": [
      "src/**/*.spec.ts"
    ]
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "_ava",
      "_typedoc_theme",
      "**/*.d.ts",
      "docs/",
      "examples/",
      "coverage/",
      "lib/",
      "webpack/",
      "src/playground",
      "server.js"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "all": true
  },
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/polyfill": "7.4.0",
    "@babel/preset-env": "^7.14.2",
    "@github/details-dialog-element": "^3.1.3",
    "@open-wc/testing": "^3.0.3",
    "@types/angular": "1.6.57",
    "@types/angular-sanitize": "^1.7.0",
    "@types/dat-gui": "0.6.3",
    "@types/dat.gui": "^0.7.5",
    "@types/jquery": "^3.3.32",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.11.2",
    "@types/offscreencanvas": "^2019.6.2",
    "@types/three": "0.103.2",
    "@types/youtube": "0.0.42",
    "@web/dev-server-esbuild": "^0.2.16",
    "@web/dev-server-legacy": "^0.1.7",
    "@web/test-runner": "^0.13.22",
    "angular": "1.7.9",
    "angular-animate": "1.7.9",
    "angular-sanitize": "^1.7.9",
    "angular-touch": "1.7.9",
    "ava": "3.15.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "browser-env": "3.2.6",
    "clean-webpack-plugin": "2.0.1",
    "concurrently": "4.1.0",
    "copy-webpack-plugin": "5.0.2",
    "css-loader": "2.1.1",
    "dat.gui": "0.7.6",
    "eslint": "^7.25.0",
    "eslint-config-google": "^0.14.0",
    "eslint-loader": "^4.0.2",
    "esm": "^3.2.25",
    "express": "4.17.1",
    "file-loader": "3.0.1",
    "gts": "^3.1.0",
    "html-webpack-plugin": "4.0.0-beta.5",
    "intersection-observer": "0.7.0",
    "lit": "^2.0.2",
    "lottie-web": "5.7.0",
    "mini-css-extract-plugin": "0.5.0",
    "npm-run-all": "4.1.5",
    "nyc": "14.1.1",
    "pixi.js": "4.8.1",
    "sass": "^1.49.7",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "three": "0.110.0",
    "ts-node": "8.1.0",
    "tslint": "5.16.0",
    "tslint-eslint-rules": "5.4.0",
    "tsup": "^6.5.0",
    "typedoc": "0.14.2",
    "typedoc-plugin-external-module-map": "1.0.0",
    "typedoc-plugin-no-inherit": "1.1.8",
    "typescript": "^4.2.4",
    "webpack": "4.29.6",
    "webpack-cli": "3.3.0",
    "webpack-dev-middleware": "3.7.0",
    "webpack-dev-server": "3.2.1",
    "webpack-merge": "4.2.1"
  },
  "scripts": {
    "build": "rm -rf lib && mkdir lib && tsup",
    "build-watch": "tsup --watch",
    "compile": "tsup",
    "coverage": "TS_NODE_PROJECT='./tsconfig.test.json' nyc ava",
    "dev-with-test": "concurrently \"tsup --watch\" \"npm run start\" \"./node_modules/.bin/ava --watch\"",
    "dev": "concurrently \"tsup --watch\" \"npm run start\"",
    "doc": "typedoc",
    "doc-server": "simplehttpserver ./docs",
    "lint": "gts lint",
    "prepare": "npm run build",
    "server": "node server",
    "start": "webpack-dev-server --open --config webpack/webpack.config.dev.cjs",
    "test": "TS_NODE_PROJECT='./tsconfig.test.json' ava && web-test-runner --node-resolve",
    "test-lit": "web-test-runner --node-resolve",
    "clean": "gts clean",
    "fix": "gts fix",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "peerDependencies": {
    "jquery": "*",
    "lit": "*",
    "lottie-web": "*"
  },
  "peerDependenciesMeta": {
    "jquery": {
      "optional": true
    },
    "lit": {
      "optional": true
    },
    "lottie-web": {
      "optional": true
    }
  }
}

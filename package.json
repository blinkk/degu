{
  "name": "@blinkk/degu",
  "version": "3.1.2",
  "description": "A general utility library for interactive websites, with a focus on performance.",
  "author": "Blinkk <hello@blinkk.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blinkk/degu.git"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "type": "module",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "default": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./lib/*": {
      "default": "./lib/*"
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rm -rf lib && mkdir lib && tsup",
    "build-watch": "tsup --watch",
    "compile": "tsup",
    "coverage": "TS_NODE_PROJECT='./tsconfig.test.json' nyc ava",
    "dev-with-test": "concurrently \"tsup --watch\" \"npm run start\" \"./node_modules/.bin/ava --watch\"",
    "dev": "concurrently \"tsup --watch\" \"npm run start\"",
    "doc": "typedoc",
    "doc-server": "simplehttpserver ./docs",
    "lint": "echo lint",
    "prepare": "npm run build",
    "examples": "vite build",
    "server": "vite",
    "start": "vite",
    "test": "TS_NODE_PROJECT='./tsconfig.test.json' ava && web-test-runner --node-resolve",
    "test-lit": "web-test-runner --node-resolve",
    "clean": "echo clean",
    "fix": "echo fix",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "ava": {
    "require": [
      "ts-node/register/transpile-only",
      "./_ava/_setup-browser-env.cjs"
    ],
    "extensions": [
      "ts"
    ],
    "files": [
      "src/**/*.spec.ts"
    ]
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "_ava",
      "_typedoc_theme",
      "**/*.d.ts",
      "docs/",
      "examples/",
      "coverage/",
      "lib/",
      "src/playground",
      "server.js"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "all": true
  },
  "devDependencies": {
    "@github/details-dialog-element": "^3.1.3",
    "@open-wc/testing": "^3.0.3",
    "@types/angular": "1.6.57",
    "@types/angular-sanitize": "^1.7.0",
    "@types/dat.gui": "^0.7.5",
    "@types/jquery": "^3.3.32",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.11.2",
    "@types/offscreencanvas": "^2019.6.2",
    "@types/three": "0.103.2",
    "@types/youtube": "0.0.42",
    "@web/dev-server-esbuild": "^0.2.16",
    "@web/dev-server-legacy": "^0.1.7",
    "@web/test-runner": "^0.13.22",
    "angular": "1.7.9",
    "angular-animate": "1.7.9",
    "angular-sanitize": "^1.7.9",
    "angular-touch": "1.7.9",
    "ava": "3.15.0",
    "browser-env": "3.2.6",
    "concurrently": "4.1.0",
    "dat.gui": "0.7.6",
    "esm": "^3.2.25",
    "fast-glob": "^3.2.12",
    "intersection-observer": "0.7.0",
    "js-beautify": "^1.14.7",
    "lit": "^2.0.2",
    "lottie-web": "5.7.0",
    "npm-run-all": "4.1.5",
    "nyc": "14.1.1",
    "pixi.js": "4.8.1",
    "sass": "^1.49.7",
    "three": "0.110.0",
    "ts-node": "8.1.0",
    "tsup": "^6.5.0",
    "typedoc": "0.14.2",
    "typedoc-plugin-external-module-map": "1.0.0",
    "typedoc-plugin-no-inherit": "1.1.8",
    "typescript": "^4.2.4",
    "vite": "^4.0.4"
  },
  "peerDependencies": {
    "angular": "*",
    "dat.gui": "*",
    "jquery": "*",
    "lit": "*",
    "lottie-web": "*",
    "pixi.js": "*",
    "three": "*"
  },
  "peerDependenciesMeta": {
    "angular": {
      "optional": true
    },
    "dat.gui": {
      "optional": true
    },
    "jquery": {
      "optional": true
    },
    "lit": {
      "optional": true
    },
    "lottie-web": {
      "optional": true
    },
    "pixi.js": {
      "optional": true
    },
    "three": {
      "optional": true
    }
  }
}
